<h3>Register for my app!</h3>
<!-- new user registration form -->
<h2>
  <%= form_for User.new do |f| %>
  <%= f.label :fname %>
  <%= f.text_field :fname %> <br><br>

  <%= f.label :lname %>
  <%= f.text_field :lname %> <br><br>

  <%= f.label :username %>
  <%= f.text_field :username %> <br><br>

  <%= f.label :email %>
  <%= f.text_field :email %> <br><br>

  <%= f.label :password %>
  <%= f.text_field :password %> <br><br>

  <%= f.submit %>
<% end %>
</h2>
